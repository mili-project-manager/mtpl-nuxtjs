[
  {
    "path": ".eslintrc.yml",
    "handlers": [
      "merge-yaml"
    ]
  },
  {
    "path": "package.json.hbs",
    "handlers": [
      "handlebars",
      "merge-json"
    ]
  },
  {
    "path": [
      ".gitignore.hbs",
      "README.md.hbs",
      "tsconfig.json.hbs"
    ],
    "handlers": [
      "handlebars"
    ]
  },
  {
    "path": [
      "components/**",
      "layouts/**",
      "middleware/**",
      "pages/**",
      "plugins/**",
      "store/**"
    ],
    "handlers": [
      "init"
    ]
  }
]
